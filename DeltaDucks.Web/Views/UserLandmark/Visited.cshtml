@model DeltaDucks.Web.ViewModels.VisitedLandmarksViewModel
@{
    ViewBag.Title = "Visited";
}

<h2 class="user-score-heading">Събрани точки: @Model.Score</h2>

@foreach (var item in Model.VisitedLandmarks)
{
    <button class="btn btn-default"
       style="color: green; display: block; margin: auto">Посетено</button>

    @Html.Partial("LandmarkPartial", item);
}



